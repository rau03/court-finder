# Database connection
MONGODB_URI=mongodb://localhost:27017/pickleball_courts

# Google Maps API Keys
# Get these from the Google Cloud Console: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your_server_side_google_maps_api_key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_client_side_google_maps_api_key

# Environment
NODE_ENV=development

# Clerk Authentication
# Get these from your Clerk dashboard: https://dashboard.clerk.com/
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# Optional Production URL for CORS headers
# PRODUCTION_URL=https://your-production-domain.com 